<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane fx:id="pane" prefHeight="400.0" prefWidth="600.0" stylesheets="@wallet.css"
            xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.WalletController">
    <VBox styleClass="box" AnchorPane.topAnchor="10" AnchorPane.leftAnchor="10" prefHeight="500">

        <Pane>
            <Label layoutX="10" layoutY="9" text="%lblId.text" />
            <Label fx:id="lblId" layoutX="124" layoutY="9" text="Label" />

            <Label layoutX="10" layoutY="34" text="%lblName.text" />
            <Label fx:id="lblNAme" layoutX="124" layoutY="9" text="Label" />

            <Label layoutX="10" layoutY="67" text="%lblCurrency.text" />
            <Label fx:id="lblCurrency" layoutX="124" layoutY="9" text="Label" />

            <Label layoutX="10" layoutY="90" text="%lblFee.text" />
            <Label fx:id="lblFee" layoutX="124" layoutY="9" text="Label" />

            <Label layoutX="10" layoutY="113" text="%lblAmount.text" />
            <Label fx:id="lblAmount" layoutX="124" layoutY="9" text="Label" />

            <Label layoutX="10" layoutY="135" text="%lblValue.text" />
            <Label fx:id="lblValue" layoutX="124" layoutY="9" text="Label" />
        </Pane>

    </VBox>
</AnchorPane>
